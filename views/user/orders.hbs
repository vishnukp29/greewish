<section class="mt-5">
    <div class="container mt-5">
        
        <table class="table table-light border border-rounded border-secondary";>
            <thead class="table-dark" >
                <tr>
                    <th scope="col">Name</th>
                    <th scope="col">Place</th>
                    <th scope="col">Payment Method</th>
                    <th scope="col">Price</th>
                    <th scope="col">Status</th>
                    

                </tr>
            </thead>
            <tbody>
                {{#each orderProduct}}
                <tr>
                    
                    <td>{{this.userAddress.UsersName}}</td>
                    <td>{{this.userAddress.Place}}</td>
                    <td>{{this.PaymentMethod}}</td>
                    <td>{{this.totalAmt}}</td>
                    <td>{{this.status}}</td>

                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</section>